    },
    "values": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::value.value",
      "mappedBy": "attribute_id"
    },
    "variant_id": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::variant.variant",
      "mappedBy": "attribute_id"
    }
  }
}
